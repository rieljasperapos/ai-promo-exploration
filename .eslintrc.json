{
  "rules": {
    "quotes": ["error", "single"],
    "no-undef": "off",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "env": {
    "jest": true,
    "node": true
  },
  "globals": {
    "module": "readonly",
    "jest": "readonly"
  },
  "plugins": ["jest", "@typescript-eslint"],
  "extends": [
    "plugin:jest/recommended",
    "plugin:@typescript-eslint/recommended"
  ]
}
